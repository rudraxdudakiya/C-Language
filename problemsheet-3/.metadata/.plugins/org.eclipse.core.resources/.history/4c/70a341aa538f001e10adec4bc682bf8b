#include<stdio.h>
#define size 10
int main()
{
	int a[size],i,sum=0;
	printf("enter %d elements\n",size);
	for(i=0;i<size;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<size;i++)
	{
		sum=sum+a[i];
	}
	printf("sum of array=%d",sum);
	return 0;
}


