#include<stdio.h>
int main()
{
	char s[50];
	printf("Enter the string=");
	scanf("%s",s);

	int length=0,i=0;
	while(s[i]!='\0')
	{
		length++;
		i++;
	}
	printf("length=%d",length);
	int pos;
	printf("\nenter the location=");
	scanf("%d",&pos);
	if(pos>length)
	{
		printf("error");
		exit(1);
	}
	for(i=length;i>=pos;i--)
	{
		s[i]=s[i-1];
	}
	printf("string after shifting=%s",s);
	return 0;
}
