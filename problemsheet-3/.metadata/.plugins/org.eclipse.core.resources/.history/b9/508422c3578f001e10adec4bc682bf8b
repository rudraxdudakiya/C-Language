#include<stdio.h>
int main()
{
	char str1[20];
	char str2[20];

	printf("enter string 1");
	scanf("%s",str1);
	printf("enter string 2");
	scanf("%s",str2);


	int i=0,length=0;
	while(str1[i]!='\0')
	{
		length++;
		i++;
	}

	i=0;
	int j=length;
	while(str2[i]!='\0')
	{
		str1[j]=str2[i];
		i++;
		j++;
	}
	str1[j]='\0';
	printf("after merging string1,string2:\n%s",str1);

	return 0;
}
