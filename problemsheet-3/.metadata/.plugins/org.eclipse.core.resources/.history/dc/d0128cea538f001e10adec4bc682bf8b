#include<stdio.h>
#define size 10
int main()
{
	int a[size],i,sum=0;

	//taking input of Array
	printf("enter %d elements\n",size);
	for(i=0;i<size;i++)
	{
		scanf("%d",&a[i]);
	}

	//printing input of Array
	printf("Arry elements\n",size);
	for(i=0;i<size;i++)
	{
		printf("%d\t",a[i]);
	}

	//calculating sum of array elements
	for(i=0;i<size;i++)
	{
		sum=sum+a[i];
	}

	//printing sum of Array elements
	printf("\nsum of array=%d",sum);
	return 0;
}


