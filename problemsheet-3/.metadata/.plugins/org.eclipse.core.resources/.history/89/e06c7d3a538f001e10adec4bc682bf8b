#include<stdio.h>
int main()
{
	char s1[20];
	char s2[20];
	int n,i=0,diff=0;
	printf("enter string1=");
	scanf("%s",s1);
	printf("enter string2=");
	scanf("%s",s2);
	printf("enter nN number of character=");
	scanf("%d",&n);
	printf("first string=%s\nsecond string=%s\n",s1,s2);

	while(i!=n)
	{
		diff=s1[i]-s2[i];
		if(diff==0)
		{
			printf("equal\n");
		}
		else
		{
			printf("not equal diff=%d",diff);
			break;
		}
	    i++;
	}

	return 0;
}

