#include<stdio.h>
#define size 10
int main()
{
	int a[size],i;
	for(i=0;i<size;i++)
	{
		printf("enter value for %d=",i);
		scanf("%d",&a[i]);
	}
	for(i=0;i<size;i++)
	{
		printf("\n Address of a[%d]=%u value of a[%d]=%d",i,&a[i],i,a[i]);
	}

	return 0;
}

